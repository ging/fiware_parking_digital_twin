<h1><%=title%></h1>
<h2>Lista stores</h2>
<ul>
    <% stores.forEach(store => { %>
        <li>
            <form method='get' action='/app/store/<%=store.id%>/till'>
                <%=store.name%>
                <input type='submit' value='Entrar'>
            </form>
        </li>
    <% }) %>
</ul>